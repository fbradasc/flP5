module led is

    with p16f84;

    _config: uint8 at 0x2007;
    led    : constant uint8 = 0x01;

    procedure Main is
        i: uint8;
        j: uint8;
    begin
        _config = _CP_OFF & _PWRTE_OFF & _WDT_OFF & _XT_OSC;

        p16f84.trisa = 0x1f; -- 00011111
        p16f84.trisb = 0xfe; -- 11111110
        asm
            bsf p16f84.portb, led.led
        end asm;
        loop
            for led.main.i in 0 to 255
            loop
                for led.main.j in 0 to 255
                loop
                    null;
                end loop;
            end loop;
            if ( ( p16f84.portb & led.led ) == 1 ) then
                asm
                    bcf p16f84.portb, led.led
                end asm;
            else
                asm
                    bsf p16f84.portb, led.led
                end asm;
            end if;
        end loop;
    end procedure;
end module;
